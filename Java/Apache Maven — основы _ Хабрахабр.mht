/* Внутренний голос */
.i-am-your-father-luke {display: block;clear:both;border: none;font: normal 40px/110% Arial,sans-serif;color: #cc9;letter-spacing: -2px;}

/* НЛО прилетело и написало */
.ufo-was-here {margin-bottom: 20px;font: normal 16px/110% Verdana,sans-serif;color:#666;}

/* Posts feed */
.posts{}
.posts .post{margin-bottom:40px;}
.posts .post.focus{}
.posts .post h1.title{color:#767676;font: 26px/118% normal Verdana, Tahoma, sans-serif;}
.posts .i-am-your-father-luke{margin-bottom:40px;}
.posts .ufo-was-here {margin-bottom: 20px;}

	
/* Single post */
.post{}
.post.special_post{background: #faf8e5;padding:7px 12px 0px;margin-left:-12px;margin-bottom:40px;}   

.post .searched-item{background: lemonChiffon;font-style: normal;}
.post .published{color:#7e7e7e;font-size: 11px;margin-bottom: 2px;}

.post h1.title{margin-bottom:8px;color:#333333;font: 30px/118% normal Verdana, Tahoma, sans-serif;}   

.post h1.title a.blog_title.corporative{color:#8277a3;}
.post h1.title a.blog_title.corporative:hover{color:#ABA4C2;}
.post h1.title a.post_title{color:#666666;}
.post h1.title a.post_title:hover{color:#a3a3a3;}
.post h1.title a.post_title:visited{color:#B5B5B5;}

.post h1.title .flag{height:18px;display: inline-block;vertical-align: top;font-size:12px;font-family: Verdana, sans-serif;;line-height: 18px;text-align: left;padding-left:5px;padding-right:10px;-webkit-border-top-left-radius: 2px;
-webkit-border-bottom-left-radius: 2px;
-moz-border-radius-topleft: 2px;
-moz-border-radius-bottomleft: 2px;
border-top-left-radius: 2px;
border-bottom-left-radius: 2px;
text-decoration: none;
}
.post h1.title .flag_translation{background: url(/images/flags.png?r=55) no-repeat right 0px;color:#5c7f95;}
.post h1.title .flag_sandbox{background: url(/images/flags.png?r=55) no-repeat right -25px;color:#8c896b;}
.post h1.title .flag_solution{background: url(/images/flags.png?r=55) no-repeat right -51px;color:#728449;}
.post h1.title .flag_recovery{background: url(/images/flags.png?r=55) no-repeat right -77px;color:#a28546;}
.post h1.title .flag_tutorial{background: url(/images/flags.png?r=55) no-repeat right -103px;color: #bb698f;}


.post h1.title a.edit{}


/* топик перевод */
.post.translation h1.title .post_title{background:url("/i/translation_icon2.gif") no-repeat left center;padding-left:30px;}
/* топик подкаст */
.post.podcast h1.title .post_title{background:url("/i/podcast_icon2.gif") no-repeat left center;padding-left:30px;}
/* топик ссылка */
.post.link h1.title .post_title{background:url("/i/link_icon2.gif") no-repeat left center;padding-left:30px;}
.post.link h1.title a.post_title{color:#749F79;}
.post.link h1.title a.post_title:hover{color:#84b18a;}

.post h1.title .locked{
  background: url("/i/lock.gif") no-repeat scroll 5px 60% #FFFFFF;
  padding-left: 15px;
}
.post h1.title .double_locked{
  background: url("/i/double-lock.gif") no-repeat scroll 5px 60% #FFFFFF;
  padding-left: 35px;
}

.post .hubs{background: url(/images/posts/hub.icon.png) no-repeat 0px 0px;padding-left:25px;font-size:11px;padding-top:2px;padding-bottom:2px;color:#999;margin-bottom:15px;}
.post .hubs.special_hubs{background: transparent;padding-left: 0;}
.post .hubs a{color:#999;}
.post .hubs a.subscribed{color:#6c9471;}
.post .hubs .profiled_hub{cursor: help;}

.post .content a:visited{color:#909;}

.post .content{font-size:13px;margin-bottom:10px;line-height:160%;font-family:Verdana, sans-serif;overflow: hidden;}

.post .content > .buttons{padding-top:20px;padding-bottom:10px;}
.post .content > .buttons a.button{}


.post .content img{max-width:100%;}
.post .content .polling{margin:20px 0;}
.post .content .polling .poll{margin-bottom:20px;}
.post .content .polling .poll:last-child{margin-bottom:0;}
.post .content .poll{}
.post .content .poll .poll_title{font-weight: bold;font-size:12px;color:#404040;}
.post .content .poll table.answer{width:auto;border:0;margin:10px 0 17px;}
.post .content .poll table.answer tr{border:0}
.post .content .poll table.answer tr td{border:0;padding:2px 0;}
.post .content .poll table.answer tr td.label{padding-left:5px;font-size:12px;}

.post .content .poll table.result{width:100%;font-size:12px;border:0;margin:10px 0 0px;}
.post .content .poll table.result tr{border:0;}
.post .content .poll table.result tr td{border:0;padding:2px 0;}
.post .content .poll table.result tr td.percent{width:1%;color:#CCCCCC;text-align: right;word-wrap:normal;word-break: normal;padding-right:10px;}
.post .content .poll table.result tr td strong{font-weight:bold;color:#404040;}
.post .content .poll table.result tr td .bar{height:5px;background:#DADACD;}
.post .content .poll table.result tr td .bar.winner{background:#7FA0B0;}

.post .content .poll .total{color:#999999;font-size:11px;padding-top:10px;font-style: italic;}

.post ul.tags{margin-bottom:15px;font-family:Verdana, sans-serif;background: url("/i/bg-tags2.gif") no-repeat scroll 0 50%; font-size:10px;padding:2px 0;padding-left:20px;}
.post ul.tags li{display:inline;color:#999999;}
.post ul.tags li a{color:#666666;}
.post ul.tags li a:hover{color:#a3a3a3;}

.post ul.tags li.fav{}
.post ul.tags li.fav a{color:#339900;}
.post ul.tags li.fav a:hover{color:#85c266;}

.post ul.tags li.favourites_edit_tags{margin-left:10px;}
.post ul.tags li.favourites_edit_tags a{color:#ccc;}
.post ul.tags li.favourites_edit_tags a:hover{color:#666;}

.abuse_wrapper{width:16px;height:16px;display: inline-block;vertical-align: middle;margin-left:5px;position: relative;}
.post .abuse{background: url('/images/abuse.png') no-repeat left top;width:16px;height:16px;display: inline-block;vertical-align: middle;margin-left:2px; }
.post .abuse:hover{ background: url('/images/abuse.png') no-repeat left bottom;cursor:pointer;}
.post .abuse.hidden{display: none;}
.post .abuse.cancel_abuse{background: url('/images/abuse.close.png') no-repeat center center;}
.post .abuse.cancel_abuse:hover{ background: url('/images/abuse.close.png') no-repeat center center;}



.post .abuse_form{
	display: inline-block;
	width:90%;
	vertical-align: middle;
	font-family:Arial,sans-serif;
	font-size:11px;
	border: 1px solid #E5E5E5;
	padding:0;
	position: relative;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	}
.post .abuse_form.hidden{display: none;}
.post .abuse_form .input{margin-right:100px;}
.post .abuse_form .input input.text{display: inline-block;vertical-align: top;
	font-size: 14px;
	margin: 0px;
	width: 100%;
	background: transparent;
	box-sizing: border-box;
	border: 0px solid #d9d9d9;
	padding: 0px;
	color: #3b3b3b;
	line-height: 29px;
	height: 29px;
	text-indent: 8px;
}




.post .abuse_form .buttons{
	position: absolute;
	right: 2px;
	top: 2px;
	display: inline-block;
}





.post .infopanel{display: inline-block;vertical-align: middle;font-family:Arial,sans-serif;font-size:11px;border: 1px solid #E5E5E5;padding:0 10px;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 5px;}
.post .infopanel.hidden{display: none;}
.post .infopanel .novote{float:left;margin-right:15px;font-size:9px;padding-top:8px;}

.post .infopanel .voting{float:left;position:relative;padding:0px 20px;margin:6px 0;margin-right:26px;}

/* еще не голосовал - не подсвечиваем ни чего. стрелки обычные, не затененные. */
.post .infopanel .voting .plus{width:11px;height:15px;display:block;position:absolute;left:0;top:0px;}
.post .infopanel .voting .minus{width:11px;height:15px;display:block;position:absolute;right:0;top:0px;}
.post .infopanel .voting a.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll left bottom transparent;}
.post .infopanel .voting a.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll -12px bottom transparent;}
.post .infopanel .voting.voted a.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll right top transparent;}
.post .infopanel .voting.voted a.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll right bottom transparent;}
.post .infopanel .voting span.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll right top transparent;}
.post .infopanel .voting span.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll right bottom transparent;}


.post .infopanel .voting a.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat 0px -16px;}
.post .infopanel .voting a.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat -12px -16px;}
.post .infopanel .voting a:hover.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat 0px 0px ;}
.post .infopanel .voting a:hover.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat -12px 0px ;}

/* проголосовал в плюс - подсвечиваем стрелку вверх и затемняем стрелку вниз*/
.post .infopanel .voting.voted_plus a.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll left top transparent;}
.post .infopanel .voting.voted_plus span.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll left top transparent;}
.post .infopanel .voting.voted_plus a.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll right bottom transparent;}
.post .infopanel .voting.voted_plus span.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll right bottom transparent;}

/* проголосовал в минус - подсвечиваем стрелку вниз и затемняем стрелку вверх */
.post .infopanel .voting.voted_minus a.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll top right transparent;}
.post .infopanel .voting.voted_minus span.plus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll top right transparent;}
.post .infopanel .voting.voted_minus a.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll -12px top transparent;}
.post .infopanel .voting.voted_minus span.minus{background:url("/i/icos/icons_vote_posts.gif?r=2") no-repeat scroll -12px top transparent;}

/* общий балл не известен */
.post .infopanel .voting .mark{float:left;color:#A9A9A9;font-weight:bold;font-size:12px;margin-top:1px;}
.post .infopanel .voting .mark span{color:#A9A9A9;}
.post .infopanel .voting .mark a{text-decoration:none;color:#A9A9A9;}
/* общий бал выше нуля */
.post .infopanel .voting .mark.positive span{color:#339900;}
/* общий балл - ниже нуля */
.post .infopanel .voting .mark.negative span{color:#CC0000;}

/* q&a */
.post .infopanel .informative{float:left;color:#e41c00;font-size:9px;text-align:left;padding:0px;padding-left:13px;margin-left:20px;line-height:28px;background: url(/images/qa.png) no-repeat 0px 6px;}
.post .infopanel .informative a{color:#e41c00;text-decoration:none;font-size:9px;}
