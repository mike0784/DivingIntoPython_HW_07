	text-indent: 1px;
	padding: 0 5px;
} 

/**
 * Иконка в кнопке
 */	
.buttons button i,
.buttons a.button i,
.buttons input i{
	vertical-align: bottom;
} 

/**
 * Большая кнопка
 */	
