.habralenta_settings .category-list .category .hubs .hub:last-child{margin-bottom:0em;}
.habralenta_settings .category-list .category .hubs .hub .checkbox{float:left;position:absolute;top:0;left:0;cursor:pointer;width:14px;height:14px;margin-top:0.1em;background:url(/i/form/checkbox.png) no-repeat -25px 0px;}
.habralenta_settings .category-list .category .hubs .hub.subscription .checkbox{background:url(/i/form/checkbox.png) no-repeat 0px 0px;}

.habralenta_settings .category-list .category.disabled .hubs .hub .checkbox{cursor:default;background:url(/i/form/checkbox.png) no-repeat -25px -75px;}
.habralenta_settings .category-list .category.disabled .hubs .hub.subscription .checkbox{background:url(/i/form/checkbox.png) no-repeat 0px -75px;}
