}
.boredLink,
.invisible,
.operationBar,
.messageOpBar
.pagerLink,
.threadIcons,
.unreadLink,
.userInfoLink,
.userRefInsertLink,
#addMessageContainer,
#authBar,
#copyrightNotice,
#logo,
#forumSupplements,
#privacyNotice,
#moderatorAdvert,
#searchBar,
#siteTitle,
#subsidiary,
#tip {
    display: none;
}
.messageHeader {
